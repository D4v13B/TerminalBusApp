{
  "expo": {
    "name": "TerminalBus",
    "slug": "TerminalBus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "terminalbus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dbusta0215.TerminalBus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-ruta-pty.png",
        "backgroundColor": "#2efae9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dbusta0215.TerminalBus",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBRGBVq1J3QnhWckNNjCmyB6uyNHSpi3v0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2efae9"
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "445b6a4b-4298-41ac-9950-5a87c9bbc443"
      }
    },
    "owner": "dbusta0215"
  }
}
